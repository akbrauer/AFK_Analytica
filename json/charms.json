			"heroes": ["Granny Dahnie", "Lumont", "Lucius"]
		},
		"7": {
			"bonus": "Increases all damage except normal attack damage by 40% for 60s. Grants the hero 35 crit for all non-normal attacks.",
			"quality": "Mythic",
			"heroes": ["Talene", "Viperian"]
		},
		"8": {
			"bonus": "Increases Crit by 100 for the next 3 hits and restores 140% HP per second over the next 6s after the hero assists or defeats an enemy, excluding summons. Grants the hero control immunity and a20% ATK boost while this effect is active.",
			"quality": "Mythic",
			"heroes": ["Shakir", "Lenya", "Seth"]
		},
		"9": {
			"bonus": "Increases damage by 1.5% and Phys & Magic DEF by 5% for every 3s the hero remains in the battle. Increases damage by 9% and Phys & Magic DEF by 30% when assisting or defeating a non-summoned enemy. The total increase from both effects is capped at 18% for damage and 60% for Phys & Magic DEF. When the limit is reached grants the hero an extra 25 Haste for the rest of the battle.",
			"quality": "Mythic",
			"heroes": ["Scarlita", "Florabelle", "Odie", "Kruger", "Korin"]
		},
		"10": {
			"bonus": "Grants 30% ATK boost for 45s to the ally placed 1 tile behind the hero when battle starts. When this ally assist or defeats an enemy other than a summon, their ATK is increased by an extra 6%, and the duration of the buff is extended by 10s.",
			"quality": "Mythic",
			"heroes": ["Reinier", "Ludovic", "Niru"]
		},
		"11": {
			"bonus": "Dispels most debuffs and makes the hero untargetable for 3s when HP drops below 35% for the first time. Grants the hero a 550% shield when the effect ends. The shield lasts for 6s, during which the hero's Life Drain is also increased by 20.",
			"quality": "Mythic",
			"heroes": ["Dionel"]
		},
		"12": {
			"bonus": "Reduces damage taken by 25% and increases their energy on hit by 30 in the first 15s of battle.",
			"quality": "Mythic",
			"heroes": ["Phraesto", "Antandra"]
		},
		"13": {
			"bonus": "Restores 60 energy per second until the hero reaches full Energy for the first time. Upon reaching full Energy for the first time, dispels control effects, grants control immunity for6s, and 25% ATK for 6s.",
			"quality": "Mythic",
			"heroes": ["Mikola", "Rowan", "Fay"]
